    href: /about/
 
---
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN"
   "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html>
<head>
<title>eeknay.net : {{ page.title }} </title>
<meta http-equiv="Content-type" content="text/html; charset=utf-8" />
<meta name="language" content="en" />
<link rel="stylesheet" type="text/css" href="/css/default.css" />
<link rel="stylesheet" type="text/css" href="/css/syntax.css" />
<link rel="alternate" type="application/rss+xml" title="RSS 2.0" href="http://eeknay.net/atom.xml" /> 
<link rel="alternate" type="text/xml" title="RSS .92" href="http://eeknay.net/atom.xml" /> 
<link rel="alternate" type="application/atom+xml" title="Atom 0.3" href="http://eeknay.net/atom.xml" /> 

</head>

{% assign navTitle = 'Blog' %}
{% for t in page.topnav %}
{% if t.text == page.title %}{% assign navTitle = page.title %}{% endif %}
{% endfor %}

<body>
<div class="page_margins">
  <div id="header">
    <h2><a href="http://blog.eeknay.net/">eeknay.net</a></h2>
    <div class="clear"></div>

    <!-- begin: main navigation #nav -->
    <div id="nav">
      <div class="hlist">
        <ul>
          {% for t in page.topnav %}
          <li{% if t.text == navTitle %} class="active"{% endif %}>
          <a href="{{ t.href }}"{% if t.title %} title="{{ t.title }}"{% endif %}>{{ t.text }}</a>
          </li>
          {% endfor %}
          <li><a href="http://github.com/eeknay">GitHub</a></li>
        </ul>
      </div>
    </div>
    <!-- end: main navigation -->
  </div>

  <!-- begin: main content area #main -->
  <div id="main">
    <div id="col1">
      <div id="col1_content">
        {{ content }}
      </div>
    </div>
    <div id="col3">
      <div id="col3_content">
        <h6 class="vlist">Work</h6>
        <ul class="vlist">
          <li>
            <a
              href="http://itsec.rwth-aachen.de"
              title="Research Group IT-Security"
              >Research Group IT-Security</a>
          </li>
          <li>
            <a
              href="http://itsec.rwth-aachen.de/people/andre-egners"
              title="Work Homepage"
              >Work Homepage</a>
          </li>
        </ul>
<!--
        <h6 class="vlist">Misc. Links</h6>
        <ul class="vlist">
          <li>
            <a
              href="http://gridengine.sunsource.net"
              >GridEngine</a>:
            <a
              href="http://gridengine.info/"
              >info</a>
            <a
              href="http://wiki.gridengine.info/wiki/"
              >wiki</a>
          </li>
          </ul>
-->
	<h6 class="vlist">Twitter</h6>
	<script src="http://widgets.twimg.com/j/2/widget.js"></script>
<script>
new TWTR.Widget({
  version: 2,
  type: 'profile',
  rpp: 4,
  interval: 30000,
  width: 'auto',
  height: 300,
  theme: {
    shell: {
      background: '#ffffff',
      color: '#000333'
    },
    tweets: {
      background: '#ffffff',
      color: '#000000',
      links: '#099330'
    }
  },
  features: {
    scrollbar: false,
    loop: false,
    live: true,
    behavior: 'all'
  }
}).render().setUser('eeknay23').start();
</script>
	</br>
      </div>
    </div>
    <div class="clear"></div>
  </div>
  <!-- end: #main -->

  <!-- begin: #footer -->
  <div id="footer">
    <div class="hlist">
      <ul>
        {% for t in page.topnav %}
        <li{% if t.text == navTitle %} class="active"{% endif %}>
        <a href="{% if t.http %}{{ t.http }}{% else %}{{ t.href }}{% endif %}"
        {% if t.title %} title="{{ t.title }}"{% endif %}
        >{{ t.text }}</a>
        {% if forloop.last == false %} | {% endif %}
        </li>
        {% endfor %}
        <li>&ndash; updated {{ site.time | date:"%Y-%m-%d" }}</li>
      </ul>
    </div>
  </div>
  <!-- end: #footer -->

</div>

</body>
</html>
